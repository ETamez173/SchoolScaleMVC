@model SchoolScoreMVC.Models.Degree
@{
    ViewData["Title"] = "DetailsModal";
}

<h1>DetailsModal</h1>


<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>

    <div class="container">
        <h2>Modal Example</h2>
        <!-- Trigger the modal with a button -->
        <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModal">Open Modal</button>

        <!-- Modal -->
        <div class="modal fade" id="myModal" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Modal Header</h4>
                    </div>
                    <div class="modal-body">
                        <p>Ranked # 4 out of 20 Fasted Growing Jobs by SomeExpertDudes Magazine.</p>
                        <dl class="row">
                            @*<dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.Id)
                            </dt>*@
                            @*<dd class="col-sm-10">
                                @Html.DisplayFor(model => model.Id)
                            </dd>*@
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.EducationName)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.EducationName)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.EarningAvg)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.EarningAvg)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.EarningHigh)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.EarningHigh)
                            </dd>
                            <dt class="col-sm-2">
                                @Html.DisplayNameFor(model => model.EarningLow)
                            </dt>
                            <dd class="col-sm-10">
                                @Html.DisplayFor(model => model.EarningLow)
                            </dd>
                        </dl>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>

    </div>

</body>
</html>
